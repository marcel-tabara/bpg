{
  "name": "bpgen",
  "version": "1.0.0",
  "author": "",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "yarn workspace @bpgen/main start",
    "server": "yarn workspace @bpgen/express start",
    "build": "yarn workspace @bpgen/main build",
